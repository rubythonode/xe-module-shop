<filter name="shop_login" module="shop" act="procShopLogin">
    <form>
        <node target="user_id" required="true" />
        <node target="password" required="true" />
    </form>
    <parameter />
    <response callback_func="completeShopLogin">
        <tag name="error" />
        <tag name="stat" />
        <tag name="message" />
    </response>
</filter>
