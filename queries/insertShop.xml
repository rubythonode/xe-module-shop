<query id="insertShop" action="insert">
    <tables>
        <table name="shop" />
    </tables>
    <columns>
        <column name="module_srl" var="module_srl" filter="number" notnull="notnull" />
        <column name="member_srl" var="member_srl" filter="number" notnull="notnull" />
        <column name="shop_content" var="shop_content" />
        <column name="profile_content" default="" var="profile_content" />
        <column name="shop_title" var="shop_title" />
		<column name="input_email" var="input_email" />
		<column name="input_website" var="input_website" />
		<column name="timezone" var="timezone" />
        <column name="regdate" var="regdate" default="curdate()" />
    </columns>
</query>
