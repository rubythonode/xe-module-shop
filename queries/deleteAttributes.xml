<query id="deleteAttributes" action="delete">
    <tables>
        <table name="shop_attributes"/>
    </tables>
    <conditions>
        <condition operation="in" column="attribute_srl" var="attribute_srls" />
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" pipe="or"/>
    </conditions>
</query>
