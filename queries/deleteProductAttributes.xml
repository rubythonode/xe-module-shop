<query id="deleteProductAttributes" action="delete">
    <tables>
        <table name="shop_product_attributes"/>
    </tables>
    <conditions>
        <condition operation="in" column="product_srl" var="product_srl" filter="number" />
        <condition operation="equal" column="attribute_srl" var="attribute_srl" filter="number" pipe="or" />
    </conditions>
</query>
