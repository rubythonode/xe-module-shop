<query id="getCartProducts" action="select">
    <tables>
        <table name="shop_shop_cart_products" />
    </tables>
    <columns></columns>
    <conditions>
        <condition operation="in" column="product_srl" var="product_srls" notnull="notnull" />
        <condition operation="equal" column="cart_srl" var="cart_srl" notnull="notnull" pipe="or" />
    </conditions>
</query>
