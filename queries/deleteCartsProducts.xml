<query id="deleteCartsProducts" action="delete">
    <tables>
        <table name="shop_cart_products"/>
    </tables>
    <conditions>
        <condition operation="in" column="cart_srl" var="cart_srls" />
        <condition operation="in" column="product_srl" var="product_srls" filter="number" pipe="or"/>
    </conditions>
</query>
