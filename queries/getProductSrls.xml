<query id="getProductSrls" action="select">
    <tables>
        <table name="shop_products" />
    </tables>
    <columns>
        <column name="product_srl" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" notnull="notnull" />
    </conditions>
</query>
