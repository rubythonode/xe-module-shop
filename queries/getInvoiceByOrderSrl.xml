<query id="getInvoiceByOrderSrl" action="select">
    <tables>
        <table name="shop_invoices" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="order_srl" var="order_srl" notnull="notnull"/>
    </conditions>
</query>
