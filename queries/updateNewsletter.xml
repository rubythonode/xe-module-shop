<query id="updateNewsletter" action="update">
    <tables>
        <table name="shop_newsletters" />
    </tables>
    <columns>
        <column name="module_srl" var="module_srl" filter="number"/>
        <column name="subject" var="subject"/>
        <column name="sender_name" var="sender_name"/>
        <column name="sender_email" var="sender_email"/>
        <column name="content" var="content"/>
    </columns>
    <conditions>
        <condition operation="equal" column="newsletter_srl" var="newsletter_srl" filter="number" notnull="notnull"/>
    </conditions>
</query>