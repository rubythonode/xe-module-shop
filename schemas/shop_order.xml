    <column name="module_srl" type="number" size="11" notnull="notnull" />
    <column name="cart_srl" type="number" size="11" />
    <column name="member_srl" type="number" size="11" />
    <column name="client_name" type="varchar" size="255"/>
    <column name="client_email" type="varchar" size="255"/>
    <column name="client_company" type="varchar" size="255"/>
    <column name="billing_address" type="text"/>
    <column name="shipping_address" type="text"/>
    <column name="payment_method" type="varchar" size="255"/>
    <column name="shipping_method" type="varchar" size="255"/>
    <column name="shipping_cost" type="number" size="11" notnull="notnull"/>
    <column name="total" type="number" size="11" notnull="notnull"/>
